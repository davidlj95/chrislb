{
  "name": "chrislb",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:public": "ng serve --host 0.0.0.0 --disable-host-check",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run chrislb:serve-ssr",
    "serve:ssr": "node dist/chrislb/server/main.js",
    "build:ssr": "ng build && ng run chrislb:server",
    "prerender": "ng run chrislb:prerender --routes-file routes-file.txt",
    "lint": "ng lint",
    "format-check": "prettier --check",
    "format-check-all": "pnpm run format-check -- .",
    "format": "prettier --ignore-unknown --write",
    "format-all": "pnpm run format -- .",
    "create-env-from-sample": "cp .env.sample .env",
    "lint-staged": "lint-staged",
    "git-hooks": "husky",
    "cms-server": "npx decap-server",
    "prebuild": "pnpm run generate-all",
    "analyze-main-bundle": "ng build --source-map && npx source-map-explorer dist/chrislb/browser/main.*.js",
    "postbuild": "cp dist/chrislb/browser/404/index.html dist/chrislb/browser/content/404.html",
    "generate-all": "tsm scripts/src/generators/all-generators.ts",
    "generate-images-lists": "tsm scripts/src/generators/images-lists-generators.ts",
    "generate-contents": "tsm scripts/src/generators/content-generators.ts",
    "generate-routes-file": "tsm scripts/src/generators/routes-file-generator.ts",
    "validate-renovate-config": "npx -p renovate renovate-config-validator",
    "lighthouse": "npx @lhci/cli autorun"
  },
  "packageManager": "pnpm@8.15.4",
  "private": true,
  "dependencies": {
    "@angular/animations": "16.2.12",
    "@angular/common": "16.2.12",
    "@angular/compiler": "16.2.12",
    "@angular/core": "16.2.12",
    "@angular/forms": "16.2.12",
    "@angular/platform-browser": "16.2.12",
    "@angular/platform-browser-dynamic": "16.2.12",
    "@angular/platform-server": "16.2.12",
    "@angular/router": "16.2.12",
    "@davidlj95/ngx-meta": "1.0.0-alpha.13",
    "@fontsource/fira-sans": "5.0.18",
    "@fortawesome/angular-fontawesome": "0.13.0",
    "@fortawesome/fontawesome-svg-core": "6.5.1",
    "@fortawesome/free-brands-svg-icons": "6.5.1",
    "@fortawesome/free-solid-svg-icons": "6.5.1",
    "@nguniversal/express-engine": "16.2.0",
    "@types/lodash-es": "4.17.12",
    "@unpic/core": "0.0.47",
    "compression": "1.7.4",
    "express": "4.18.3",
    "gardevoir": "1.0.0",
    "lodash-es": "4.17.21",
    "rxjs": "7.8.1",
    "swiper": "11.0.7",
    "tslib": "2.6.2",
    "zone.js": "0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.2.12",
    "@angular-eslint/builder": "16.3.1",
    "@angular-eslint/eslint-plugin": "16.3.1",
    "@angular-eslint/eslint-plugin-template": "16.3.1",
    "@angular-eslint/schematics": "16.3.1",
    "@angular-eslint/template-parser": "16.3.1",
    "@angular/cli": "16.2.12",
    "@angular/compiler-cli": "16.2.12",
    "@lhci/cli": "0.12.0",
    "@nguniversal/builders": "16.2.0",
    "@types/compression": "1.7.5",
    "@types/express": "4.17.21",
    "@types/jasmine": "5.1.4",
    "@types/node": "20.10.8",
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "@typescript-eslint/parser": "7.1.0",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "husky": "9.0.11",
    "imagekit": "5.0.0",
    "jasmine-core": "5.1.2",
    "karma": "6.4.3",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "lint-staged": "15.2.2",
    "ng-mocks": "14.12.1",
    "prettier": "3.2.5",
    "tsm": "2.3.0",
    "typescript": "5.1.6"
  }
}
