{
  "name": "chrislb",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:public": "ng serve --host 0.0.0.0 --disable-host-check",
    "build": "ng build",
    "postbuild": "cp dist/chrislb/browser/404/index.html dist/chrislb/browser/content/404.html",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "format:check": "prettier --check",
    "format:check:all": "pnpm run format:check -- .",
    "format": "prettier --ignore-unknown --write",
    "format:all": "pnpm run format -- .",
    "create-env-from-sample": "cp .env.sample .env",
    "lint-staged": "lint-staged",
    "git-hooks": "husky",
    "cms-server": "npx decap-server",
    "analyze-main-bundle": "ng build --source-map && npx source-map-explorer dist/chrislb/browser/main.*.js",
    "generate": "tsm scripts/src/generators/all-generators.ts",
    "generate:images-lists": "tsm scripts/src/generators/images-lists-generators.ts",
    "generate:contents": "tsm scripts/src/generators/content-generators.ts",
    "generate:routes-file": "tsm scripts/src/generators/routes-file-generator.ts",
    "validate-renovate-config": "npx -p renovate renovate-config-validator",
    "lighthouse-ci": "npx @lhci/cli autorun",
    "serve:ssr:chrislb": "node dist/chrislb/server/server.mjs"
  },
  "packageManager": "pnpm@9.9.0",
  "private": true,
  "dependencies": {
    "@angular/animations": "18.2.1",
    "@angular/common": "18.2.1",
    "@angular/compiler": "18.2.1",
    "@angular/core": "18.2.1",
    "@angular/forms": "18.2.1",
    "@angular/platform-browser": "18.2.1",
    "@angular/platform-browser-dynamic": "18.2.1",
    "@angular/platform-server": "18.2.1",
    "@angular/router": "18.2.1",
    "@angular/ssr": "18.2.1",
    "@davidlj95/ngx-meta": "1.0.0-beta.11",
    "@fontsource/fira-sans": "5.0.21",
    "@fortawesome/angular-fontawesome": "0.15.0",
    "@fortawesome/fontawesome-svg-core": "6.6.0",
    "@fortawesome/free-brands-svg-icons": "6.6.0",
    "@fortawesome/free-solid-svg-icons": "6.6.0",
    "@unpic/core": "0.0.49",
    "compression": "1.7.4",
    "express": "4.19.2",
    "gardevoir": "1.0.0",
    "lodash-es": "4.17.21",
    "rxjs": "7.8.1",
    "swiper": "11.1.12",
    "tslib": "2.7.0",
    "zone.js": "0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.1",
    "@angular-eslint/builder": "18.3.0",
    "@angular-eslint/eslint-plugin": "18.3.0",
    "@angular-eslint/eslint-plugin-template": "18.3.0",
    "@angular-eslint/schematics": "18.3.0",
    "@angular-eslint/template-parser": "18.3.0",
    "@angular/cli": "18.2.1",
    "@angular/compiler-cli": "18.2.1",
    "@lhci/cli": "0.14.0",
    "@types/compression": "1.7.5",
    "@types/express": "4.17.21",
    "@types/jasmine": "5.1.4",
    "@types/lodash-es": "4.17.12",
    "@types/node": "20.16.2",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "browser-sync": "3.0.2",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "husky": "9.1.5",
    "imagekit": "5.1.0",
    "jasmine-core": "5.2.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "lint-staged": "15.2.9",
    "ng-mocks": "14.13.1",
    "prettier": "3.3.3",
    "tsm": "2.3.0",
    "typescript": "5.5.4"
  }
}
